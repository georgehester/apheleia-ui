  ],
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^5.2.5",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^6.1.4",
    "jsmin": "^1.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.2.4",
    "typescript-plus": "^3.1.5",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
