{
  "name": "apheleia-ui",
  "version": "0.1.0",
  "description": "Css and Javascript Ui library.",
  "main": "index.ts",
  "scripts": {
    "build-sass-development": "webpack --config webpack.sass.development.js",
    "build-sass-production": "webpack --config webpack.sass.production.js",
    "build-ts-production": "tsc --project tsconfig.json && minify-js-production",
    "build-ts-development": "tsc --project tsconfig.json --watch",
    "minify-js-production": "jsmin -o ./build/apheleia/build.min.js ./build/apheleia/build.js"
  },
